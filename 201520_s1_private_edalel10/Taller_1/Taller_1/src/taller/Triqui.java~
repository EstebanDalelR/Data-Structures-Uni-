  package taller;
  
  public class Triqui
  {
    
    public Triqui()
    {
      int [][] tablero= new int [3][3];
    //aparece llena de 0       
    }
    //-1 será O y 1 será X
    public void recibirJugada (int pos1,int pos2, int turno)
    {
      if (turno==1)//entra el turno en x
      {
        tablero[pos1][pos2]=1;
      }
      else//entra el turno en o
      {
        tablero[pos1][pos2]=-1;
      }
    } 
